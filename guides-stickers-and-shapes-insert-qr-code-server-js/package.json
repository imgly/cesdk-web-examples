{
  "name": "guides-stickers-and-shapes-insert-qr-code-server-js",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "secrets": "bash scripts/setup-secrets.sh",
    "dev": "vite-node server-js.ts",
    "prebuild": "npm run check:all",
    "build": "tsc --noEmit",
    "build:local": "CESDK_USE_LOCAL=true vite build",
    "check:syntax": "tsc --project tsconfig.base.json --noEmit",
    "check:format": "prettier --check \"**/*.{ts,js,json}\"",
    "check:lint": "eslint \"**/*.{ts,js}\"",
    "check:all": "npm run check:syntax && npm run check:format && npm run check:lint",
    "fix:format": "prettier --write \"**/*.{ts,js,json}\"",
    "fix:lint": "eslint --fix \"**/*.{ts,js}\"",
    "fix:all": "npm run fix:format && npm run fix:lint",
    "test:unit": "exit 0",
    "test:integration": "exit 0",
    "test:component": "exit 0",
    "test:e2e": "exit 0",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:component && npm run test:e2e",
    "qa": "npm run build && npm run test:all",
    "qa:local": "npm run build:local && npm run test:all",
    "ci": "npm run qa:local",
    "dev:local": "CESDK_USE_LOCAL=true vite-node server-js.ts",
    "postinstall": "npm run tsconfig:local || true",
    "tsconfig:local": "node ../shared/scripts/generate-local-tsconfig.mjs || node scripts/generate-empty-tsconfig-local.mjs"
  },
  "dependencies": {
    "@cesdk/node": "latest",
    "dotenv": "^16.0.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^46.0.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.3",
    "vite-node": "^2.0.0"
  }
}
