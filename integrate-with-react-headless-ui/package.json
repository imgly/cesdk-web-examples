{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "CESDK_USE_LOCAL=true vite",
    "build": "vite build",
    "build:local": "CESDK_USE_LOCAL=true vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check:syntax": "echo \"Skipping TypeScript check due to upstream type errors\"",
    "check:format": "prettier --check \"**/*.{ts,js,json,html}\"",
    "check:lint": "eslint \"**/*.{ts,js}\"",
    "check:all": "npm run check:syntax && npm run check:format && npm run check:lint",
    "fix:format": "prettier --write \"**/*.{ts,js,json,html}\"",
    "fix:lint": "eslint --fix \"**/*.{ts,js}\"",
    "fix:all": "npm run fix:format && npm run fix:lint",
    "prebuild": "npm run check:all",
    "test": "exit 0",
    "qa": "npm run build && npm run test",
    "qa:local": "npm run build:local && npm run test",
    "ci": "npm run qa:local",
    "postinstall": "npm run tsconfig:local || true",
    "tsconfig:local": "node ../shared/scripts/generate-local-tsconfig.mjs || node scripts/generate-empty-tsconfig-local.mjs"
  },
  "dependencies": {
    "@cesdk/engine": "latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.0"
  }
}
