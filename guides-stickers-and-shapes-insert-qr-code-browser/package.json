{
  "name": "guides-stickers-and-shapes-insert-qr-code-browser",
  "version": "1.0.0",
  "description": "CE.SDK guide: Insert QR codes as vector shapes",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:local": "VITE_CESDK_LOCAL=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "check:syntax": "tsc --project tsconfig.base.json --noEmit",
    "check:lint": "eslint \"**/*.{ts,js}\"",
    "check:format": "prettier --check \"**/*.{ts,js,json,html,css}\"",
    "fix:format": "prettier --write \"**/*.{ts,js,json,html,css}\"",
    "postinstall": "node scripts/generate-empty-tsconfig-local.mjs",
    "tsconfig:local": "node ../../../scripts/cesdk.ts.link.mjs guides-stickers-and-shapes-insert-qr-code-browser",
    "capture:hero_image": "node scripts/capture-hero.mjs"
  },
  "dependencies": {
    "@cesdk/cesdk-js": "latest",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.0.0",
    "sharp": "^0.33.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.3"
  }
}
